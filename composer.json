{
    "name"              : "jbzoo/path",
    "type"              : "library",
    "description"       : "PHP virtual path library",
    "license"           : "MIT",
    "keywords"          : ["url", "jbzoo", "path", "realpath", "filesystem", "virtual path"],
    "minimum-stability" : "stable",
    "authors"           : [
        {
            "name"  : "Sergey Kalistratov",
            "email" : "kalistratov.s.m@gmail.com",
            "role"  : "developers"
        },
        {
            "name"  : "SmetDenis",
            "email" : "denis@jbzoo.com",
            "role"  : "lead"
        }
    ],
    "require"           : {
        "php"         : ">=5.3.10",
        "jbzoo/utils" : "^1.6|^2.0"
    },
    "require-dev"       : {
        "jbzoo/phpunit"      : "^1.7|1.x-dev",
        "symfony/filesystem" : "^2.8",
        "jbzoo/profiler"     : "^1.0"
    },
    "autoload"          : {
        "psr-4" : {
            "JBZoo\\Path\\" : "src"
        }
    },
    "extra"             : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    },
    "support"           : {
        "issues" : "https://github.com/JBZoo/Path/issues"
    }
}
